{
    "manifest_version": 3,
    "name": "Website Usage Tracker",
    "version": "1.0",
    "description": "Analyzes your website usage and suggests productive alternatives.",
    "permissions": [
        "storage",
        "history",
        "tabs",
        "activeTab"
    ],
    "action": {
        "default_popup": "page1.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
