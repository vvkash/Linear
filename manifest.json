{
    "manifest_version": 3,
    "name": "Website Usage Tracker",
    "version": "1.0",
    "description": "Analyzes your website usage and suggests productive alternatives.",
    "permissions": [
      "storage",
      "history",
      "tabs",
      "activeTab"
    ],
    "action": {
      "default_popup": "page2S.html"
    },
    "background": {
      "service_worker": "page2S.js"
    },
    "icons": {
      
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
  