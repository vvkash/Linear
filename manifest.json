{
    "manifest_version": 3,
    "name": "Website Usage Tracker",
    "version": "1.0",
    "description": "Analyzes your website usage and suggests productive alternatives.",
    "permissions": [
        "storage",
        "history",
        "tabs",
        "activeTab"
    ],
    "action": {
        "default_popup": "p1.html"
    },
    "background": {
        "service_worker": "p1.js"   
    },
    "icons": {
      
    }
}