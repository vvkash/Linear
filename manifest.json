{
    "manifest_version": 3,
    "name": "Linear: Lock In",
    "version": "1.0",
    "description": "Track and limit browsing ",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }